{
  "name": "node-express-boilerplate",
  "version": "1.0.0",
  "description": "RESTful API Node Server Boilerplate",
  "main": "src/index.js",
  "repository": "https://github.com/hahnavi/node-express-boilerplate.git",
  "author": "Abdul Munif Hanafi <amunifhanafi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "sequelize": "cross-env NODE_ENV=development dotenv sequelize",
    "test": "npm run test:prepare:db && jest -i --colors --verbose --detectOpenHandles",
    "test:prepare:db": "cross-env NODE_ENV=test dotenv sequelize db:migrate:undo:all && cross-env NODE_ENV=test dotenv sequelize db:migrate",
    "coverage": "jest -i --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "node",
    "node.js",
    "express",
    "rest",
    "api",
    "postgresql",
    "sequelize"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.5.0",
    "http-status": "^1.5.0",
    "joi": "^17.4.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.5.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.25.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  }
}
